{
  "name": "HysteriaHeroku",
  "description": "Hysteria on heroku",
  "keywords": ["Hysteria"],
  "env": {
    "port": {
      "description": "Hysteria开启的端口",
      "value": "33445"
    },
    "domain": {
      "description": "输入您的域名，不输入则默认自签wechat.com证书",
      "value": "wechat.com"
    },
    "protocol": {
      "description": "协议类型：1-udp(QUIC)，2-faketcp，3-wechat-video",
      "value": "3"
    },
    "download": {
      "description": "客户端期望的下行速度(默认50mbps)",
      "value": "50"
    },
    "upload": {
      "description": "客户端期望的上行速度(默认10mbps)",
      "value": "10"
    },
    "auth_str": {
      "description": "认证口令",
      "value": ""
    }
  },
  "website": "https://github.com/yuehen7/HerokuHysteria",
  "repository": "https://github.com/yuehen7/HerokuHysteria",
  "stack": "container"
}
